{
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": [true, "match-declaration-order"],
    "no-bitwise": false,
    "indent": [
      true,
      "spaces",
      2
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ]
  },
  "rulesDirectory": []
}
